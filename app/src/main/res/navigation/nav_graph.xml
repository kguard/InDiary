<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_gragh"
    app:startDestination="@id/personFragment"
    >
    <fragment
        android:id="@+id/personFragment"
        android:name="com.kguard.indiary.PersonFragment"
        android:label="fragment_people"
        tools:layout="@layout/fragment_person" >
        <action
            android:id="@+id/action_personFragment_to_addPersonFragment"
            app:destination="@id/addPersonFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_enter_anim"
            app:popExitAnim="@anim/nav_default_exit_anim"
            app:popUpTo="@id/personFragment"
            app:popUpToInclusive="false" />
        <action
            android:id="@+id/action_personFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/memoryFragment"
        android:name="com.kguard.indiary.MemoryFragment"
        android:label="fragment_memory"
        tools:layout="@layout/fragment_memory" >
        <action
            android:id="@+id/action_memoryFragment_to_addMemoryFragment"
            app:destination="@id/addMemoryFragment" />
        <action
            android:id="@+id/action_memoryFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.kguard.indiary.DetailFragment"
        android:label="fragment_detail"
        tools:layout="@layout/fragment_detail" >
        <action
            android:id="@+id/action_detailFragment_to_detailPersonFragment"
            app:destination="@id/detailPersonFragment" />
        <action
            android:id="@+id/action_detailFragment_to_detailMemoryFragment"
            app:destination="@id/detailMemoryFragment" />
        <argument
            android:name="person_id"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/detailPersonFragment"
        android:name="com.kguard.indiary.DetailPersonFragment"
        android:label="fragment_detail_person"
        tools:layout="@layout/fragment_detail_person" >
        <action
            android:id="@+id/action_detailPersonFragment_to_updatePersonFragment"
            app:destination="@id/updatePersonFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailMemoryFragment"
        android:name="com.kguard.indiary.DetailMemoryFragment"
        android:label="fragment_detail_memory"
        tools:layout="@layout/fragment_detail_memory" >
        <action
            android:id="@+id/action_detailMemoryFragment_to_detailMemory2Fragment"
            app:destination="@id/detailMemory2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/addPersonFragment"
        android:name="com.kguard.indiary.AddPersonFragment"
        android:label="fragment_add_person"
        tools:layout="@layout/fragment_add_person" >
        <action
            android:id="@+id/action_addPersonFragment_to_addCharacterDialog"
            app:destination="@id/addCharacterDialog" />
    </fragment>
    <fragment
        android:id="@+id/addMemoryFragment"
        android:name="com.kguard.indiary.AddMemoryFragment"
        android:label="fragment_add_memory"
        tools:layout="@layout/fragment_add_memory" />
    <fragment
        android:id="@+id/detailMemory2Fragment"
        android:name="com.kguard.indiary.DetailMemory2Fragment"
        android:label="fragment_detail_memory2"
        tools:layout="@layout/fragment_detail_memory2" >
        <action
            android:id="@+id/action_detailMemory2Fragment_to_updateMemoryFragment"
            app:destination="@id/updateMemoryFragment" />
    </fragment>
    <fragment
        android:id="@+id/updatePersonFragment"
        android:name="com.kguard.indiary.UpdatePersonFragment"
        android:label="fragment_update_person"
        tools:layout="@layout/fragment_update_person" />
    <fragment
        android:id="@+id/updateMemoryFragment"
        android:name="com.kguard.indiary.UpdateMemoryFragment"
        android:label="fragment_update_memory"
        tools:layout="@layout/fragment_update_memory" />
    <dialog
        android:id="@+id/addCharacterDialog"
        android:name="com.kguard.indiary.AddCharacterDialog"
        android:label="AddCharacterDialog" />
</navigation>